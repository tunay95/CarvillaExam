@using CarVillaExam.Areas.Admin.ViewModels;
@model CreateServiceVM

<div class="container">
	<form method="post">
		<div class="mb-3" >
			<label asp-for="Title" class="form-label">Title</label>
			<input asp-for="Title" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
			<span asp-validation-for="Title" style="color:red"></span>
		</div>
		<div class="mb-3">
			<label asp-for="Description" class="form-label">Description</label>
			<input asp-for="Description" class="form-control" id="exampleInputPassword1">
			<span asp-validation-for="Description" style="color:red"></span>
		</div>
		<div class="mb-3">
			<label asp-for="Icon">Icon</label>
			<input class="form-control" asp-for="Icon">
			<span asp-validation-for="Icon" style="color:red"></span>
		</div>
		<br>
		<button type="submit" class="btn btn-primary">Submit</button>
	</form>
</div>